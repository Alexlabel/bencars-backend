{
  "kind": "collectionType",
  "collectionName": "models",
  "info": {
    "singularName": "model",
    "pluralName": "models",
    "displayName": "Model"
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    },
    "content-type-builder": {
      "visible": true
    }
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "models"
    },
    "gallery": {
      "type": "json",
      "default": []
    }
  }
}


